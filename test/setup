#!/bin/bash
mkdir -p /home/travis/
cp -r $1/test/home/ /home/travis/test-home/

cp -r $1/test/remote-test-repo/ /home/travis/remote-test-repo
cd /home/travis/remote-test-repo
git init
git add bar/permissions.json bar/docker-image/Dockerfile
git commit -m 'test'
cd /home/travis/test-home/.subuser/repositories/default
git init
git add .
git commit -m 'test'
